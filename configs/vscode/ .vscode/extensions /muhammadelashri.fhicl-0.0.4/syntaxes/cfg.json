{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "JobSub Configuration",
    "patterns": [
        {
            "include": "#basic"
        }
    ],
    "repository": {
        "basic": {
            "patterns": [
                {
                    "match": "(-{2}\\w\\S*=|-{2}\\w*|\\s-\\w)",
                    "name": "constant.numeric.cfg   "
                }
            ]
        },
    "scopeName": "source.cfg"
}
}